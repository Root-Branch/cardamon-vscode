{
  "name": "cardamon-toml-manager",
  "displayName": "Cardamon TOML Manager",
  "description": "This extension provides tools to manage TOML configuration files for Cardamon",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "cardamon-sidebar-view",
          "title": "Cardamon",
          "icon": "./src/assets/images/icon.svg"
        }
      ]
    },
    "views": {
      "cardamon-sidebar-view": [
        {
          "type": "webview",
          "id": "cardamon-sidebar",
          "name": "Cardamon",
          "icon": "./src/assets/images/icon.svg",
          "contextualTitle": "Cardamon"
        }
      ]
    },
    "commands": [
      {
        "command": "cardamon-toml-manager.showSidebar",
        "title": "Show Cardamon Sidebar"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "concurrently \"tsc -p ./tsconfig.extension.json --skipLibCheck\" \"vite build\" \"npx tailwindcss -i ./src/assets/css/main.css -o ./dist/output.css\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.extension.json --skipLibCheck\" \"vite build --watch\" \"npx tailwindcss -i ./src/assets/css/main.css -o ./dist/output.css --watch\"",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint \"src/**/*.{ts,vue}\"",
    "test": "vscode-test"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@iarna/toml": "^2.2.5",
    "@vitest/ui": "^0.26.2",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^6.6.1",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.19",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^20.0.3",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "vite": "^2.9.14",
    "vitest": "^0.26.2",
    "vue-tsc": "^0.40.6"
  }
}
